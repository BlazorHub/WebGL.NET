#version 300

uniform mat4 worldViewProj;

attribute vec3 in_var_NORMAL;
attribute vec3 in_var_POSITION;
attribute vec2 in_var_TEXCOORD;

varying vec3 out_var_NORMAL;
varying vec2 out_var_TEXCOORD;

void main()
{
    gl_Position = Matrices.worldViewProj * vec4(in_var_POSITION, 1.0);
    out_var_NORMAL = in_var_NORMAL;
    out_var_TEXCOORD = in_var_TEXCOORD;
}

